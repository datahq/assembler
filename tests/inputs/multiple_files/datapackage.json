{
  "name": "multiple-files",
  "resources": [
    {
      "name": "birthdays",
      "path": "data/birthdays.csv",
      "format": "csv",
      "schema": {
        "fields": [
          {
            "name": "date",
            "type": "date"
          },
          {
            "name": "first_name",
            "type": "string"
          },
          {
            "name": "last_name",
            "type": "string"
          }
        ],
        "primaryKey": "date"
      }
    },
    {
      "name": "emails",
      "path": "data/emails.csv",
      "format": "csv",
      "schema": {
        "fields": [
          {
            "name": "id",
            "type": "number"
          },
          {
            "name": "email",
            "type": "string"
          }
        ]
      }
    }
  ]
}
